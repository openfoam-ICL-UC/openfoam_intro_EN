FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.01;
    }
    
    outlet
    {
        type            zeroGradient;
    }

    bottom
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }

    heater
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }

    top
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }

    front
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }

    back
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
}


